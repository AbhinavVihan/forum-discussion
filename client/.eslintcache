rrors, tags } = this.state;\n    return (\n      <React.Fragment>\n        <ToastContainer />\n        <div className=\"container-lg\">\n          <h1>Create a New Discussion</h1>\n          <div\n            className=\"container-lg-6 m-4 p-4 rounded\"\n            style={{ backgroundColor: \"#F1F1F1\" }}\n          >\n            <form onSubmit={this.handleSubmit}>\n              <Input\n                value={data.title}\n                onChange={this.handleChange}\n                label=\"Title\"\n                name=\"title\"\n                type=\"text\"\n                error={errors.title}\n              />\n              <div className=\"form-group\">\n                <label htmlFor=\"description\">Description</label>\n                <textarea\n                  value={data.description}\n                  onChange={this.handleChange}\n                  name=\"description\"\n                  type=\"description\"\n                  id=\"description\"\n                  className=\"form-control\"\n                />\n                {errors.description && (\n                  <div className=\"alert-info\">{errors.description}</div>\n                )}\n              </div>\n              <div className=\"form-group\">\n                <label htmlFor=\"tags\">Related Tags</label>\n                <br />\n                {tags.map((tag) => (\n                  <React.Fragment>\n                    <label className=\"mr-3 ml-3\">\n                      <input\n                        key={tag._id}\n                        className=\"form-check-input\"\n                        type=\"checkbox\"\n                        onChange={() => this.handleTagChange(tag._id)}\n                      />\n                      {tag.name}\n                    </label>\n                  </React.Fragment>\n                ))}\n                {errors.tags && <div className=\"alert-info\">{errors.tags}</div>}\n              </div>\n              <div className=\"text-center\">\n                <button\n                  className=\"btn btn-primary mt-4\"\n                  disabled={this.validate()}\n                >\n                  Submit\n                </button>\n              </div>\n            </form>\n          </div>\n        </div>\n      </React.Fragment>\n    );\n  }\n}\n\nexport default NewPost;\n","/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/components/log.jsx",[],"/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/components/common/input.jsx",[],"/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/components/register.jsx",[],"/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/components/common/form.jsx",[],"/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/services/httpService.js",["102"],"import axios from \"axios\";\nimport { toast } from \"react-toastify\";\nimport \"react-toastify/dist/ReactToastify.css\";\n\naxios.defaults.headers.common[\"x-auth-token\"] = localStorage.getItem(\"token\");\n\naxios.interceptors.response.use(null, (error) => {\n  const expectedError =\n    error.response &&\n    error.response.status >= 400 &&\n    error.response.status < 500;\n\n  if (!expectedError) {\n    toast(\"An unexpected error occured!\");\n  }\n  console.log(error);\n  return Promise.reject(error);\n});\n\nexport default {\n  get: axios.get,\n  post: axios.post,\n  put: axios.put,\n  delete: axios.delete,\n};\n","/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/services/userService.js",[],"/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/services/authService.js",[],"/home/dev/Desktop/Studies/Projects/full-stack-application/client/src/components/logout.jsx",["103"],"import React, { Component } from \"react\";\n\nclass Logout extends Component {\n  componentDidMount() {\n    localStorage.removeItem(\"token\");\n    window.location = \"/dashboard\";\n  }\n  render() {\n    return null;\n 